{
  "name": "@botpress/studio",
  "version": "13.0.0",
  "description": "Create your virtual assistants",
  "repository": "https://github.com/botpress/studio.git",
  "author": "Botpress, Inc.",
  "license": "AGPL-3.0",
  "scripts": {
    "eslint": "eslint packages/ --ext .ts,.tsx",
    "prettier": "prettier --check './packages/**/*.ts(x)' '!**/*.d.ts'",
    "reset": "git clean -xdf",
    "build": "yarn workspaces foreach -tiv run build",
    "watch": "yarn workspaces foreach -iv run watch",
    "clean": "yarn workspaces foreach -pi run clean",
    "start": "yarn workspace @botpress/studio-be start",
    "test:unit": "jest -c jest.unit.config.ts"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.18.1",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "37.7.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "27.1.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "12.18.1"
  },
  "packageManager": "yarn@3.1.1",
  "resolutions": {
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11"
  }
}
